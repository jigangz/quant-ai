# ===================================
# Quant-AI Environment Variables
# ===================================
# Copy this file to .env and fill in your values

# ===== Environment =====
ENV=dev
DEBUG=true

# ===== Database =====
# Local development (Docker)
DATABASE_URL=postgresql+psycopg://quant:quant@localhost:5432/quantdb
# Or SQLite for quick testing
# DATABASE_URL=sqlite:///./quant.db

# ===== Supabase (optional) =====
# Used for model registry in production
# SUPABASE_URL=https://your-project.supabase.co
# SUPABASE_KEY=your-anon-key
# SUPABASE_SERVICE_KEY=your-service-key

# ===== Storage =====
STORAGE_BACKEND=local
STORAGE_LOCAL_PATH=./artifacts
# For S3:
# STORAGE_BACKEND=s3
# S3_BUCKET=your-bucket
# S3_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your-key
# AWS_SECRET_ACCESS_KEY=your-secret

# ===== Data Providers =====
PROVIDERS_ENABLED=market
MARKET_PROVIDER=yahoo
# For Polygon.io:
# POLYGON_API_KEY=your-key

# ===== Feature Engineering =====
DEFAULT_FEATURE_GROUPS=ta_basic,volatility

# ===== Job Queue (V3) =====
REDIS_URL=redis://localhost:6379/0

# ===== Model Training =====
DEFAULT_MODEL_TYPE=logistic
DEFAULT_HORIZON_DAYS=5
DEFAULT_LABEL_TYPE=direction
TRAINING_MAX_WORKERS=4

# ===== API =====
API_PREFIX=/api/v1
CORS_ORIGINS=http://localhost:3000,http://localhost:5173
RATE_LIMIT_PER_MINUTE=60
REQUEST_TIMEOUT_SECONDS=300

# ===== Logging =====
LOG_LEVEL=INFO
LOG_FORMAT=text

# ===== LLM (V3) =====
# LLM_PROVIDER=openai
# OPENAI_API_KEY=sk-...

# ===== Vector Store (V3) =====
# VECTOR_BACKEND=faiss
# PINECONE_API_KEY=...
# PINECONE_ENVIRONMENT=...
